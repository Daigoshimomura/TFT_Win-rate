{
  "name": "nextjs-typescript-styled-components",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next -p 8000",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "vercel --prod",
